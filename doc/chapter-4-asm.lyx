	seq 		
\end_layout

\begin_layout Plain Layout

		selezionati(RISCIACQUO) := 0
\end_layout

\begin_layout Plain Layout

		selezionati(RULLI) := 0
\end_layout

\begin_layout Plain Layout

		selezionati(CERCHI) := 0
\end_layout

\begin_layout Plain Layout

