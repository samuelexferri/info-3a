rule r_LAVORAZIONE =
\end_layout

\begin_layout Plain Layout

 	if (selezionati(RISCIACQUO) = 1) then
\end_layout

\begin_layout Plain Layout

		stato := FRISCIACQUO
\end_layout

\begin_layout Plain Layout

	else
\end_layout

\begin_layout Plain Layout

		if (selezionati(RULLI) = 1) then
\end_layout

\begin_layout Plain Layout

			stato := FRULLI
\end_layout

\begin_layout Plain Layout

		else
\end_layout

\begin_layout Plain Layout

			if (selezionati(CERCHI) = 1) then
\end_layout

\begin_layout Plain Layout

				stato := FCERCHI
\end_layout

\begin_layout Plain Layout

			else
\end_layout

\begin_layout Plain Layout

				if (selezionati(ASCIUGATURA) = 1) then
\end_layout

\begin_layout Plain Layout

					stato := FASCIUGATURA
\end_layout

\begin_layout Plain Layout

				else
\end_layout

\begin_layout Plain Layout

					stato := START
\end_layout

\begin_layout Plain Layout

				endif
\end_layout

\begin_layout Plain Layout

			endif
\end_layout

\begin_layout Plain Layout

		endif
\end_layout

