\series bold
r_MENU:
\end_layout

\begin_layout Standard
Questa regola permette di settare a 1 il valore delle fasi selezionate dall'uten
te.
 Si passa allo stato 
\emph on
PAGAMENTO
\emph default
 solo quando l'utente clicca 
\emph on
FINESELEZIONE
\emph default
.
 Prima di cambiare stato viene anche calcolata la 
\emph on
sommaselezioni 
\emph default
utile in seguito per il calcolo del costo totale del lavaggio.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

rule r_MENU =
\end_layout

\begin_layout Plain Layout

	let ($s = selezione) in
\end_layout

\begin_layout Plain Layout

		if ($s != FINESELEZIONE) then
\end_layout

\begin_layout Plain Layout

			selezionati($s) := 1
\end_layout

\begin_layout Plain Layout

		else
\end_layout

\begin_layout Plain Layout

			par
\end_layout

\begin_layout Plain Layout

				sommaselezioni := selezionati(RISCIACQUO) + selezionati(RULLI) + selezionati
(CERCHI) + selezionati(ASCIUGATURA)
\end_layout

\begin_layout Plain Layout

				stato := PAGAMENTO
\end_layout

\begin_layout Plain Layout

			endpar
\end_layout

\begin_layout Plain Layout

		endif
\end_layout

\begin_layout Plain Layout

	endlet
\end_layout

\end_inset


\end_layout

